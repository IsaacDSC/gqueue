{
  "name": "payment.charged",
  "type": "external",
  "triggers": [
    {
      "service_name": "external-service",
      "type": "persistent",
      "host": "http://localhost:3333",
      "path": "/payment/charged",
      "headers": {
        "Content-Type": "application/json"
      },
      "option": {
        "wq_type": "low_throughput",
        "max_retries": 3,
        "retention": "168h",
        "unique_ttl": "60s"
      }
    }
  ]
}